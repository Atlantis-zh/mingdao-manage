<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mingdao.dao.base.IUserInfoDao">
	<resultMap id="userResultMap" type="com.mingdao.domain.UserInfo">
		<id property="id" javaType="long" jdbcType="BIGINT" column="id" />
		<result property="userName" javaType="string" jdbcType="VARCHAR" column="username"/>
		<result property="passWord" javaType="string" jdbcType="VARCHAR" column="password"/>
		<result property="nickName" javaType="string" jdbcType="VARCHAR" column="nickname"/>
		<result property="userCode" javaType="string" jdbcType="VARCHAR" column="usercode"/>
		<result property="storeId" javaType="long" jdbcType="BIGINT" column="storeid"/>
		<result property="status" javaType="int" jdbcType="INTEGER" column="status"/>
		<result property="phone" javaType="string" jdbcType="VARCHAR" column="phone"/>
		<result property="email" javaType="string" jdbcType="VARCHAR" column="email"/>
		<result property="creator" javaType="long" jdbcType="BIGINT" column="creator"/>
		<result property="modifiedTime" javaType="java.sql.Timestamp" jdbcType="TIMESTAMP" column="modifiedtime"/>
		<result property="modifier" javaType="long" jdbcType="BIGINT" column="modifier"/>
		<result property="createTime" javaType="java.sql.Timestamp" jdbcType="TIMESTAMP" column="createtime"/>
	</resultMap>

	<insert id="insertVO" parameterType="com.mingdao.domain.UserInfo" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO userinfo
		(usercode,storeid,username,password,nickname,status,phone,email,creator,createtime,modifier,modifiedtime)
		VALUES
		(#{userCode},#{storeId},#{userName},#{passWord},#{nickName},#{status},#{phone},#{email},#{creator},#{createTime},#{modifier},#{modifiedTime})
	</insert>
	
	
	<update id="updateVO"  parameterType="com.mingdao.domain.UserInfo">
		UPDATE userinfo SET
			<if test="passWord!=null">password=#{passWord},</if>
			<if test="nickName!=null">nickname=#{nickName},</if>
			<if test="userCode!=null">usercode=#{userCode},</if>
			<if test="userName!=null">username=#{userName},</if>
			<if test="storeId">storeid=#{storeId},</if>
			<if test="status!=null">status=#{status},</if>
			<if test="phone!=null">phone=#{phone},</if>
			<if test="email!=null">email=#{email},</if>
			<if test="creator!=null">creator=#{creator},</if>
			<if test="modifiedTime!=null">modifiedtime=#{modifiedTime},</if>
			<if test="modifier!=null">modifier=#{modifier},</if>
			<if test="createTime!=null">createtime=#{createTime},</if>
		WHERE id=#{id}
	</update>	

	<select id="pageQueryByCondition" parameterType="java.util.Map" resultMap="userResultMap">
		select
			t.id,
			t.storeid,
			t.username,
			t.password,
			t.nickname,
			t.usercode,
			t.status,
			t.phone,
			t.email,
			t.creator,
			t.modifiedtime,
			t.modifier,
			t.createtime
		from  userinfo t where 1=1
           <if test="id != null">
              and t.id=#{id}
           </if>
           <if test="storeId != null">
              and t.storeid=#{storeId}
           </if>
           <if test="userCode != null">
              and t.usercode=#{userCode}
           </if>
           <if test="userName != null">
              and t.username=#{userName}
           </if>
           <if test="status != null">
              and t.status=#{status}
           </if>           
	</select>

	<select id="getCountByCondition" parameterType="java.util.Map" resultType="int">
		select
		 count(1)
		from  userinfo t where 1=1
           <if test="id != null">
              and t.id=#{id}
           </if>
           <if test="storeId != null">
              and t.storeid=#{storeId}
           </if>
           <if test="userCode != null">
              and t.usercode=#{userCode}
           </if>
           <if test="userName != null">
              and t.username=#{userName}
           </if>
           <if test="status != null">
              and t.status=#{status}
           </if>  
	</select>

    <delete id="deleteDocById" parameterType="long">
        delete from userinfo  where id = #{id}
    </delete>

	<select id="queryById" parameterType="long" resultMap="userResultMap">
	    SELECT
			t.id,
			t.storeid,
			t.username,
			t.password,
			t.nickname,
			t.usercode,
			t.status,
			t.phone,
			t.email,
			t.creator,
			t.modifiedtime,
			t.modifier,
			t.createtime
        FROM  userinfo t where id=#{id}
	</select>

	<select id="singleQueryByCondition" parameterType="java.util.Map" resultMap="userResultMap">
	    SELECT
			t.id,
			t.storeid,
			t.username,
			t.password,
			t.nickname,
			t.usercode,
			t.status,
			t.phone,
			t.email,
			t.creator,
			t.modifiedtime,
			t.modifier,
			t.createtime
		FROM userinfo t where 1=1
           <if test="id != null">
              and t.id=#{id}
           </if>
           <if test="storeId != null">
              and t.storeid=#{storeId}
           </if>
           <if test="userCode != null">
              and t.usercode=#{userCode}
           </if>
           <if test="userName != null">
              and t.username=#{userName}
           </if>
           <if test="status != null">
              and t.status=#{status}
           </if>
           <if test="phone != null">
              and t.phone=#{phone}
           </if>           
	</select>


</mapper>