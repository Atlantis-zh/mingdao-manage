<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mingdao.domain.UserInfo">
	<insert id="insertVO" parameterType="com.mingdao.domain.UserInfo">
		INSERT INTO userinfo
		(usercode,username,password,nickname,status,phone,email,creator,createtime,modifier,modifiedtime)
		VALUES
		(#{userCode},#{userName},#{passWord},#{nickName},#{status},#{phone},#{email},#{creator},#{createtime},#{modifier},#{modifiedtime})
	</insert>

</mapper>