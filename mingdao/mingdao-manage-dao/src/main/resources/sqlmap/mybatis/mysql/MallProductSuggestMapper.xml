<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yonyou.nccloud.dao.search.IMallProductSuggestDao">

	<select id="queryAll" resultType="com.alibaba.fastjson.JSONObject">
		select * from mall_product_suggest
	</select>
	
	<select id="queryByPage" parameterType="com.yonyou.nccloud.search.vo.Page" resultType="com.alibaba.fastjson.JSONObject">
		SELECT id,name,specify
		FROM
		mall_product_suggest
		limit #{from},#{pageSize}
	</select>
</mapper>